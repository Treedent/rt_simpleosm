{
    "name": "syradev/rt_simpleosm",
    "type": "typo3-cms-extension",
    "description": "Insert a simple OpenStreetMap. No API key required!",
    "keywords": [
        "OpenStreetMap", "leaflet", "fullscreen", "minimap", "open", "street", "map"
    ],
    "authors": [
        {
            "name": "Regis TEDONE",
            "email": "regis.tedone@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Treedent/rt_simpleosm/issues"
    },
    "license": ["GPL-2.0-or-later"],
    "require": {
        "typo3/cms-core": "~8.7.32 || ~9.5.23 || ~10.4.12"
    },
    "autoload": {
        "psr-4": {
            "SYRADEV\\RtSimpleosm\\": "Classes"
        }
    },
    "replace": {
        "syradev/rt_simpleosm": "*",
        "typo3-ter/rt_simpleosm": "*"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rt_simpleosm"
        }
    }
}